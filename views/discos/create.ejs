<form action="/discos" method="POST" enctype="multipart/form-data">
    <label for="titulo">Título:</label>
    <input type="text" id="titulo" name="titulo" required>

    <label for="dataLanc">Data de Lançamento:</label>
    <input type="date" id="dataLanc" name="dataLanc" required>

    <label for="artistaId">Artista:</label>
    <select name="artistaId" id="artistaId" required>
        <!-- Suponha que você tenha artistas no banco de dados -->
        <option value="1">Artista 1</option>
        <option value="2">Artista 2</option>
    </select>

    <label for="generoId">Gênero:</label>
    <select name="generoId" id="generoId" required>
        <!-- Suponha que você tenha gêneros no banco de dados -->
        <option value="1">Gênero 1</option>
        <option value="2">Gênero 2</option>
    </select>

    <label for="capa">Capa:</label>
    <input type="file" id="capa" name="capa">

    <button type="submit">Criar Disco</button>
</form>